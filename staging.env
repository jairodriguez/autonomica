# Staging Environment Configuration
# This file contains environment variables for the staging environment
# Copy this file to .env.staging and modify as needed

# Environment
NODE_ENV=staging
ENVIRONMENT=staging
ENV=staging

# Application Configuration
APP_NAME=Autonomica
APP_VERSION=0.1.0
APP_ENV=staging
DEBUG=false
LOG_LEVEL=info

# Server Configuration
HOST=0.0.0.0
PORT=8000
FRONTEND_PORT=3000
WORKER_PORT=8001

# Database Configuration
DATABASE_URL=postgresql://staging_user:staging_password@staging-db:5432/autonomica_staging
DATABASE_HOST=staging-db
DATABASE_PORT=5432
DATABASE_NAME=autonomica_staging
DATABASE_USER=staging_user
DATABASE_PASSWORD=staging_password
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20
DATABASE_POOL_TIMEOUT=30

# Redis Configuration
REDIS_URL=redis://staging-redis:6379/0
REDIS_HOST=staging-redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_POOL_SIZE=20

# OpenAI Configuration
OPENAI_API_KEY=sk-staging-key-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7
OPENAI_TIMEOUT=60

# Clerk Authentication
CLERK_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
CLERK_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
CLERK_JWT_ISSUER=https://clerk.staging.autonomica.ai
CLERK_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# External APIs
TWITTER_API_KEY=staging_twitter_key
TWITTER_API_SECRET=staging_twitter_secret
TWITTER_ACCESS_TOKEN=staging_access_token
TWITTER_ACCESS_TOKEN_SECRET=staging_access_token_secret

LINKEDIN_CLIENT_ID=staging_linkedin_client_id
LINKEDIN_CLIENT_SECRET=staging_linkedin_client_secret
LINKEDIN_ACCESS_TOKEN=staging_linkedin_access_token

# Email Configuration
SMTP_HOST=smtp.staging.autonomica.ai
SMTP_PORT=587
SMTP_USER=staging@autonomica.ai
SMTP_PASSWORD=staging_smtp_password
SMTP_TLS=true
SMTP_FROM=staging@autonomica.ai

# Monitoring and Logging
SENTRY_DSN=https://staging_sentry_dsn@sentry.io/staging_project
LOGFLARE_API_KEY=staging_logflare_key
LOGFLARE_SOURCE_TOKEN=staging_source_token

# Performance and Caching
CACHE_TTL=300
SESSION_TTL=3600
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900

# Security
SECRET_KEY=staging_secret_key_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
JWT_SECRET=staging_jwt_secret_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ENCRYPTION_KEY=staging_encryption_key_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
CORS_ORIGINS=https://staging.autonomica.ai,https://staging-frontend.autonomica.ai

# Feature Flags
FEATURE_AI_CHAT=true
FEATURE_WEB_SCRAPING=true
FEATURE_SOCIAL_MEDIA=true
FEATURE_DATA_ANALYSIS=true
FEATURE_MULTI_AGENT=true
FEATURE_ADVANCED_ANALYTICS=false

# Testing and Development
TESTING=false
MOCK_EXTERNAL_APIS=true
ENABLE_DEBUG_ENDPOINTS=false
ENABLE_SWAGGER_UI=true
ENABLE_REDOC=true

# Backup and Storage
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=7
STORAGE_BUCKET=autonomica-staging-backups
STORAGE_REGION=us-east-1

# Third-party Services
GOOGLE_ANALYTICS_ID=GA_STAGING_ID
MIXPANEL_TOKEN=staging_mixpanel_token
INTERCOM_APP_ID=staging_intercom_app_id
ZENDESK_DOMAIN=staging.zendesk.com
ZENDESK_EMAIL=staging@autonomica.ai
ZENDESK_API_TOKEN=staging_zendesk_token

# Webhook URLs
WEBHOOK_SLACK_URL=https://hooks.slack.com/services/staging/staging/staging
WEBHOOK_DISCORD_URL=https://discord.com/api/webhooks/staging/staging
WEBHOOK_TEAMS_URL=https://staging.webhook.office.com/webhookb2/staging/staging

# Timezone and Locale
TZ=UTC
LOCALE=en_US
DATE_FORMAT=YYYY-MM-DD
TIME_FORMAT=HH:mm:ss

# Health Check
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10
HEALTH_CHECK_RETRIES=3