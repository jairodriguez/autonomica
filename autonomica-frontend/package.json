{
  "name": "autonomica-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:integration": "node run_integration_tests.js",
    "test:integration:watch": "jest --testPathPattern=integration --watch",
    "test:integration:coverage": "jest --testPathPattern=integration --coverage",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:typescript": "tsc --noEmit",
    "lint:stylelint": "stylelint \"**/*.{css,scss}\"",
    "lint:stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "lint:all": "npm run lint:eslint && npm run lint:typescript && npm run lint:stylelint",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "quality": "npm run lint:all && npm run format:prettier:check && npm run test:ci",
    "quality:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix && npm run format:prettier",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@clerk/nextjs": "^6.22.0",
    "@heroicons/react": "^2.2.0",
    "ai": "^4.3.16",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "next": "15.3.3",
    "openai": "^5.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.0.0",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.5",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^6.13.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "stylelint": "^16.0.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-prettier": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "web-vitals": "^3.5.0",
    "cross-env": "^7.0.3"
  }
}
